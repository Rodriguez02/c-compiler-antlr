    "version": "0.2.0",
    "configurations": [
        {
            "type": "antlr-debug",
            "request": "launch",
            "name": "Debug Current Grammar",
            "input": "src/app/ejemplo.txt",
            "visualParseTree": true
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-App<ProyectSimbols>",
            "request": "launch",
            "mainClass": "app.App",
            "projectName": "ProyectSimbols"
        }
    ]
}